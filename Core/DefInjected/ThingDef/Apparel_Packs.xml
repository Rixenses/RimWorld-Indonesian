  <!-- EN: firefoam pop pack -->
  <Apparel_FirefoampopPack.label>Sabuk busa</Apparel_FirefoampopPack.label>
  <!-- EN: A utility pack made for firefighters. When triggered, a burst of fire-retardant foam is released, covering a wide circle around its wearer. -->
  <Apparel_FirefoampopPack.description>Ransel utilitarian untuk petugas pemadam kebakaran.Saat diaktifkan, semburan busa firewall dilepaskan yang mencakup lingkaran lebar di sekitar pembawanya.</Apparel_FirefoampopPack.description>
  <!-- EN: firefoam pop -->
  <Apparel_FirefoampopPack.comps.CompApparelReloadable.chargeNoun>Beban busa</Apparel_FirefoampopPack.comps.CompApparelReloadable.chargeNoun>
  <!-- EN: on cooldown -->
  <Apparel_FirefoampopPack.comps.CompApparelReloadable.cooldownGerund>dalam pendinginan</Apparel_FirefoampopPack.comps.CompApparelReloadable.cooldownGerund>
  <!-- EN: pop firefoam -->
  <Apparel_FirefoampopPack.verbs.pop_firefoam.label>menjatuhkan</Apparel_FirefoampopPack.verbs.pop_firefoam.label>
  
  <!-- EN: smokepop pack -->
  <Apparel_SmokepopBelt.label>sabuk asap</Apparel_SmokepopBelt.label>
